<div id="<%= dom_id password %>">
  <p>
    <strong>Url:</strong>
    <%= password.url %>
  </p>

  <p>
    <strong>Username:</strong>
    <%= password.username %>
  </p>

  <p>
    <strong>Password:</strong>
    <%= password.password %>
  </p>

  <p>
    <strong>Expiration date:</strong>
    <%= password.expiration_date %>
  </p>

  <p>
    <strong>Category path:</strong>
    <%= password.category_path %>
  </p>

  <p>
    <strong>Tags:</strong>
    <%= password.tags %>
  </p>

  <p>
    <strong>Favorite:</strong>
    <%= password.favorite %>
  </p>

  <p>
    <strong>Notes:</strong>
    <%= password.notes %>
  </p>

</div>
